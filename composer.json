{
    "name": "farmaciasdirect/sauron-laravel",
    "description": "Laravel SDK for Sauron logging API - process logs, user audit logs, and request logs",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Farmacias Direct",
            "email": "dev@farmaciasdirect.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "FarmaciasDirect\\Sauron\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FarmaciasDirect\\Sauron\\SauronServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
